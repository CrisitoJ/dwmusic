# PKGBUILD

pkgname=dwmusic
pkgver=1
pkgrel=1
pkgdesc="CLI for custom DWM themes. For each theme, it includes palette colors and wallpapers"
arch=("x86_64")
license=('GPLv3')
depends=('feh')
noextract=()
validpgpkeys=()
source=("dwmusic.sh")

sha256sums=("6fde09bb5543eff731a17d039311395a7315e48ea95bb749e8db917af6d660dd")  

package() {
	install -Dm755 "${srcdir}/dwmusic.sh" "${pkgdir}/usr/bin/dwmusic"
}
