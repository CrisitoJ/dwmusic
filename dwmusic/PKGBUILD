# PKGBUILD

pkgname=dwmusic
pkgver=1
pkgrel=1
pkgdesc="CLI for custom DWM themes. For each theme, it includes palette colors and wallpapers"
arch=("x86_64")
license=('GPLv3')
depends=('feh')
noextract=()
validpgpkeys=()
source=("dwmusic.sh")

sha256sums=("fa8d6419f99358d8139ceec64bf04dcf049c044f0f9f292d5a6691cd688f287f")  

package() {
	install -Dm755 "${srcdir}/dwmusic.sh" "${pkgdir}/usr/bin/dwmusic"
}
